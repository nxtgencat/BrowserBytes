{
  "manifest_version": 3,
  "name": "BrowserBytes",
  "version": "1.0",
  "description": "Import and export local storage and cookies with ease",
  "permissions": [
    "activeTab",
    "cookies",
    "tabs",
    "downloads",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}